{
  "$schema": "https://json.schemastore.org/package",
  "name": "template-nodejs",
  "version": "1.0.0",
  "main": "dist/index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "tsc --project tsconfig.build.json",
    "format:check": "prettier ./ --check",
    "format:write": "prettier ./ --write",
    "lint": "eslint ./src/ ./specs/",
    "test": "node -r ts-node/register --test ./specs/**/*.spec.ts",
    "test:watch": "node -r ts-node/register --watch --test ./specs/**/*.spec.ts",
    "watch": "node -r ts-node/register --watch src/index.ts",
    "dev": "node -r ts-node/register src/index.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@swc/core": "^1.6.13",
    "@swc/helpers": "^0.5.11",
    "@tsconfig/node22": "^22.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.8",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.3.2",
    "regenerator-runtime": "^0.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.0.0-alpha.33"
  }
}
